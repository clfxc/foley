// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
    output   = "../.generated/client"
}

datasource db {
    provider = "sqlite"
    url      = "file:./db.sqlite"
}

model Post {
    id      String @id @default(cuid())
    title   String
    content String
}
